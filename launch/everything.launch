<launch>
    <rosparam param="PLAYING_ROBOTS"> [1, 1] </rosparam>
    <rosparam param="/walls">{'left': -5.0, 'right': 5.0, 'up': 5.0, 'down': -5.0}</rosparam>

    <include file="$(find randgen_omni_dataset)/launch/landmarks.launch"/>
    <node name="ball" pkg="randgen_omni_dataset" type="ball"/>
    
    <node name="OMNI1" pkg="randgen_omni_dataset" type="robot" output="screen">
        <rosparam param="initial_pose"> {'x': -2, 'y': 0.5, 'theta': 0}</rosparam>
    </node>
    <node name="OMNI1_odometry" pkg="randgen_omni_dataset" type="odometry" output="screen">
        <param name="topic" value="/OMNI1/genOdometry"/>
    </node>

    <node name="OMNI2" pkg="randgen_omni_dataset" type="robot" output="screen">
        <rosparam param="initial_pose"> {'x': 1, 'y': 0.5, 'theta': 3.14}</rosparam>
    </node>
    <node name="OMNI2_odometry" pkg="randgen_omni_dataset" type="odometry" output="screen">
        <param name="topic" value="/OMNI2/genOdometry"/>
    </node>

</launch>
