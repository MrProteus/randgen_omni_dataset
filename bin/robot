#! /usr/bin/env python

import rospy
from math import pi
from randgen_omni_dataset import robot

LOG_LEVEL = rospy.DEBUG

if __name__ == '__main__':

    name = 'OMNI1'
    rospy.init_node(name+'_robot', log_level=LOG_LEVEL)

    initial_pose = {'x': 0.5,
                    'y': 2.0,
                    'theta': pi/3.0}
    robot = robot.Robot(initial_pose, name)
    robot.run(True)
    try:
        robot.loop()
    except rospy.ROSInterruptException:
        pass
